{
  "conn": {
    "mysql_main_schema": "enigma",
    "mysql_conn_str": "ENV:ENIGMA_MYSQL_CONN_STR",
    "minio_main_bucket": "enigma",
    "minio_endpoint": "ENV:ENIGMA_MINIO_ENDPOINT",
    "minio_access_key": "ENV:ENIGMA_MINIO_ACCESS_KEY",
    "minio_secret_key": "ENV:ENIGMA_MINIO_SECRET_KEY"
  },
  "runtime": {
    "tmpPath": "tmp"
  },
  "resources": {
    "capecXMLList": {
      "url": "https://capec.mitre.org/data/xml/views/3000.xml.zip",
      "fileName": "3000.xml",
      "category": "capec"
    }
  },
  "schemas": {
    "gprof": {
      "gprofprojects": {
        "path": "database/enigma/tables/gprof/gprofprojects.sql",
        "version": 0.1
      },
      "gproflinks": {
        "path": "database/enigma/tables/gprof/gproflinks.sql",
        "version": 0.1
      },
      "gprofnodes": {
        "path": "database/enigma/tables/gprof/gprofnodes.sql",
        "version": 0.1
      }
    }
  }
}